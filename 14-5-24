<body>
    <h1>Jadwal Pelajaran</h1><h1>X RPL 3 (umum)</h1>
    <table>
        <thead>
            <tr>
                <th>Hari</th>
                <th>Senin</th>
                <th>Selasa</th>
                <th>Rabu</th>
                <th>Kamis</th>
                <th>Jum'at</th>
            </tr>
        </thead>
        <tbody id="jadwal-table">
        </tbody>
    </table>
    <script>
        const jadwal = {
            "Jam 1": ["Bahasa Inggris", "IPAS", "Seni Budaya", "Bahasa Inggris", "PPKn"],
"Jam 2": ["Bahasa Inggris", "IPAS", "Seni Budaya", "Bahasa Inggris", "PPKn"],
"Jam 3": ["PJOK", "Matematika", "Bahasa Indonesia", "Seni Budaya", "PPKn"],
"Jam 4": ["PJOK", "Matematika", "BK", "Seni Budaya", "IPAS"],
"Jam 5": ["Matematika", "Bahasa Indonesia", "Bahasa Inggris", "PJOK", "IPAS"],
"Jam 6": ["Matematika", "Bahasa Indonesia", "Bahasa Inggris", "PJOK", "PABP"],
"Jam 7": ["Bahasa Indonesia", "Simdig", "PABP", "PJOK", "PABP"],
"Jam 8": ["Bahasa Indonesia", "Simdig", "PABP", "Bahasa Jawa", "P5"],
"Jam 9": ["Matematika", "Simdig", "Sejarah", "Bahasa Jawa", "P5"],
"Jam 10": ["Matematika", "Simdig", "Sejarah", "Bahasa Jawa", "P5"]
        };
        const tbody = document.getElementById('jadwal-table');

        for (const [hari, pelajaran] of Object.entries(jadwal)) {
            const row = document.createElement('tr');
            const hariCell = document.createElement('td');
            hariCell.textContent = hari;
            row.appendChild(hariCell);

            pelajaran.forEach(pelajaranItem => {
                const pelajaranCell = document.createElement('td');
                pelajaranCell.textContent = pelajaranItem;
                row.appendChild(pelajaranCell);
            });

            for (let i = pelajaran.length; i < 10; i++) 
        tbody.appendChild(row);
        }
    </script>
